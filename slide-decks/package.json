{
  "name": "support-cours",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "slidev": "slidev",
    "project_design": "slidev --open project_design.md",
    "java": "slidev --open java.md",
    "spring_boot": "slidev --open spring_boot.md",
    "nestjs": "slidev --open nestjs.md",
    "docker": "slidev --open docker.md",
    "build-project_design": "slidev build -o dist/project-design project_design.md",
    "build-java": "slidev build -d -o dist/java java.md",
    "build-spring_boot": "slidev build -d -o dist/spring-boot spring_boot.md",
    "build-nestjs": "slidev build -d -o dist/nestjs nestjs.md",
    "build-docker": "slidev build -d -o dist/docker docker.md",
    "export": "slidev export"
  },
  "slidev": {
    "addons": [
      "slidev-component-progress",
      "slidev-addon-qrcode",
      "slidev-addon-java-runner"
    ]
  },
  "dependencies": {
    "@slidev/cli": "^52.1.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "sanitize-html": "^2.17.0",
    "slidev-addon-java-runner": "^0.1.1",
    "slidev-addon-qrcode": "^1.0.2",
    "slidev-component-progress": "^1.1.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/sanitize-html": "^2.16.0",
    "@unocss/preset-wind3": "^66.4.2",
    "playwright-chromium": "^1.55.0"
  }
}