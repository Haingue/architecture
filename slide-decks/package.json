{
  "name": "support-cours",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "slidev": "slidev",
    "project_design": "slidev --open course_project-design.md",
    "java": "slidev --open course_java.md",
    "spring_boot": "slidev --open course_spring-boot.md",
    "nestjs": "slidev --open course_nestjs.md",
    "docker": "slidev --open course_docker.md",
    "monitoring": "slidev --open course_monitoring.md",
    "build-project_design": "slidev build -o dist/project-design course_project-design.md",
    "build-java": "slidev build -d -o dist/java course_java.md",
    "build-spring_boot": "slidev build -d -o dist/spring-boot course_spring-boot.md",
    "build-nestjs": "slidev build -d -o dist/nestjs course_nestjs.md",
    "build-docker": "slidev build -d -o dist/docker course_docker.md",
    "build-monitoring": "slidev build -d -o dist/monitoring course_monitoring.md",
    "export": "slidev export"
  },
  "slidev": {
    "addons": [
      "slidev-component-progress",
      "slidev-addon-qrcode",
      "slidev-addon-java-runner"
    ]
  },
  "dependencies": {
    "@slidev/cli": "^52.1.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "sanitize-html": "^2.17.0",
    "slidev-addon-java-runner": "^0.1.1",
    "slidev-addon-qrcode": "^1.0.2",
    "slidev-component-progress": "^1.1.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/sanitize-html": "^2.16.0",
    "@unocss/preset-wind3": "^66.4.2",
    "playwright-chromium": "^1.55.0"
  }
}